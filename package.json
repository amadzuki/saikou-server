{
  "name": "saikou-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pm2:dev": "pm2 startOrGracefulReload ecosystem.config.js --env development",
    "pm2:staging": "pm2 startOrGracefulReload ecosystem.config.js --env staging",
    "pm2:production": "pm2 startOrGracefulReload ecosystem.config.js --env production",
    "pm2:list": "pm2 list",
    "pm2:logs": "pm2 logs template-api",
    "pm2:flush": "pm2 flush template-api",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "pm2:deploy:setup": "pm2 deploy ecosystem.config.js production setup",
    "pm2:deploy": "pm2 deploy ecosystem.config.js production",
    "debug": "NODE_ENV=development nodemon src/index.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint . --ext .js",
    "postinstall": "./setup.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amadzuki/saikou-server.git"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "clean-css": "^4.1.11",
    "constantinople": "^3.1.1",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "express": "~4.17.1",
    "http-errors": "~1.8.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "mongoose-sequence": "^5.2.2",
    "morgan": "~1.10.0",
    "uglify-js": "^2.6.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4"
  }
}
