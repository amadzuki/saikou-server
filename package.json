    "express": "~4.17.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.15",
    "mongoose-sequence": "^5.3.1",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
